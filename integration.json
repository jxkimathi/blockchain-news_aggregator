{
  "name": "Blockchain News Aggregator",
  "type": "interval",
  "descriptions": "Fetches and sends the latest blockchain news articles to the channel at regular intervals.",
  "settings": [
    {
      "key": "interval",
      "type": "number",
      "label": "Interval (minutes)",
      "default": 60,
      "required": true
    },
    {
      "key": "source",
      "type": "string",
      "label": "News Source",
      "default": "blockchain",
      "required": true
    }
  ],
  "permissions": ["send_messages"],
  "data": {}
}
